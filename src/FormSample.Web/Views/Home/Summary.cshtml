@model FormSample.Web.Controllers.Data
@{
Layout = null;
}

<h2>Summary</h2>
<table>
    <tr>
        <td>First Name</td>
        <td>@Model.FirstName</td>
    </tr>
    <tr>
        <td>Last Name</td>
        <td>@Model.LastName</td>
    </tr>
    <tr>
        <td>Phone</td>
        <td>@Model.Phone</td>
    </tr>
    <tr>
        <td>Address</td>
        <td>@Model.Address</td>
    </tr>
    <tr>
        <td>City</td>
        <td>@Model.City</td>
    </tr>
    <tr>
        <td>State</td>
        <td>@Model.State</td>
    </tr>
    <tr>
        <td>Zip</td>
        <td>@Model.Zip</td>
    </tr>
</table>
